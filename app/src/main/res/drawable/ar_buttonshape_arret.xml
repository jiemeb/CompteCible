<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <padding
                android:bottom="3dp"
                android:left="3dp"
                android:right="3dp"
                android:top="3dp" />

            <size
                android:width="0dp"
                android:height="0dp" />

            <stroke
                android:width="1dp"
                android:color="#878787" />
        </shape>
    </item>
    <item android:drawable="@drawable/ic_arret" />
</layer-list>
